<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="workbench"
		class="org.jacp.project.JACP.UnitTests.workbench.UnitTestWorkBench">
		<property name="perspectives">
			<list>
				<ref bean="perspectiveOne" />
			</list>
		</property>
	</bean>
	<bean id="perspectiveOne"
		class="org.jacp.project.JACP.UnitTests.perspecitve.UnitTestPerspectiveServiceWrapper">
		<property name="name" value="perspectiveOne" />
		<property name="id" value="id01" />
		<property name="active" value="true" />
		<property name="subcomponents">
			<list>
				<ref bean="piStateful" />

			</list>
		</property>
	</bean>
	<bean id="piStateful"
		class="org.jacp.project.JACP.UnitTests.service.PiStatefulServiceOne"
		scope="prototype">
		<property name="name" value="piStateful" />
		<property name="active" value="true" />
		<property name="id" value="id001" />
	</bean>
</beans>