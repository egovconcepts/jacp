<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="workbench" class="org.jacp.swing.demo1.workbench.TestSwingWorkbench">

		<property name="perspectives">
			<list>
				<ref bean="perspective3" />
				<ref bean="perspective1" />
				<ref bean="perspective2" />
			</list>
		</property>
	</bean>

	<bean id="perspective1"
		class="org.jacp.swing.demo1.perspectives.TestSwingSplitPanePerspective">
		<property name="name" value="testPerspective" />
		<property name="id" value="id01" />
		<property name="active" value="true" />
		<property name="subcomponents">
			<list>
				<ref bean="editor1" />
				<ref bean="editor2" />
				<ref bean="editor3" />
				<ref bean="editor8" />
				<ref bean="ping" />
				<ref bean="pong" />
			</list>
		</property>
	</bean>
	<bean id="perspective2"
		class="org.jacp.swing.demo1.perspectives.TestSwingSplitPanePerspective2">
		<property name="name" value="testPerspective2" />
		<property name="id" value="id02" />
		<property name="active" value="false" />
		<property name="subcomponents">
			<list>
				<ref bean="editor4" />
				<ref bean="editor5" />

			</list>
		</property>
	</bean>

	<bean id="perspective3"
		class="org.jacp.swing.demo1.perspectives.DemoMessagePerformancePerspective">
		<property name="name" value="PerformanceDemo" />
		<property name="id" value="id01_1" />
		<property name="active" value="true" />
		<property name="subcomponents">
			<list>
				<ref bean="editor6" />
				<ref bean="editor7" />
				<ref bean="editor9" />
				<ref bean="editor10" />
				<ref bean="editor11" />
			</list>
		</property>
	</bean>

	<bean id="editor1" class="org.jacp.swing.demo1.editors.TestSwingEditor">
		<property name="name" value="testEditor1" />
		<property name="active" value="true" />
		<property name="id" value="id03" />
		<property name="executionTarget" value="editor0" />
	</bean>

	<bean id="editor2" class="org.jacp.swing.demo1.editors.TestSwingEditor2">
		<property name="name" value="testEditor2" />
		<property name="active" value="true" />
		<property name="id" value="id04" />
		<property name="executionTarget" value="editor1" />
	</bean>
	<bean id="editor3" class="org.jacp.swing.demo1.editors.TestEditor3">
		<property name="name" value="testEditor3" />
		<property name="active" value="true" />
		<property name="id" value="id05" />
		<property name="executionTarget" value="editor1" />
	</bean>
	<bean id="editor4" class="org.jacp.swing.demo1.editors.TestEditor4">
		<property name="name" value="testEditor4" />
		<property name="active" value="true" />
		<property name="id" value="id06" />
		<property name="executionTarget" value="view" />
	</bean>
	<bean id="editor5" class="org.jacp.swing.demo1.editors.TestEditor5">
		<property name="name" value="testEditor5" />
		<property name="active" value="false" />
		<property name="id" value="id07" />
		<property name="executionTarget" value="editor0" />
	</bean>

	<bean id="editor6"
		class="org.jacp.swing.demo1.editors.DemoMessagePerformanceEditorProducer">
		<property name="name" value="testEditor6" />
		<property name="active" value="true" />
		<property name="id" value="id08" />
		<property name="executionTarget" value="editor0" />
	</bean>

	<bean id="editor7"
		class="org.jacp.swing.demo1.editors.DemoMessagePerformanceEditorConsumer">
		<property name="name" value="testEditor7" />
		<property name="active" value="true" />
		<property name="id" value="id09" />
		<property name="executionTarget" value="editor1" />
	</bean>

	<bean id="editor8"
		class="org.jacp.swing.demo1.editors.DemoBackgroundMessagePerformanceEditor">
		<property name="name" value="testEditor8" />
		<property name="active" value="true" />
		<property name="id" value="id10" />
		<!-- <property name="executionTarget" value="editor1" /> -->
	</bean>
	<bean id="editor9"
		class="org.jacp.swing.demo1.editors.DemoBackgroundMessagePerformanceEditor2">
		<property name="name" value="testEditor9" />
		<property name="active" value="true" />
		<property name="id" value="id11" />
		<!-- <property name="executionTarget" value="editor1" /> -->
	</bean>
	<bean id="editor10" class="org.jacp.swing.demo1.editors.StatelessDemoConsumer"
		scope="prototype">
		<property name="name" value="testEditor10" />
		<property name="active" value="true" />
		<property name="id" value="id12" />
		<property name="testBean" ref="testBean" />
	</bean>
	<bean id="editor11" class="org.jacp.swing.demo1.editors.StatelessRecursiveDemoConsumer"
		scope="prototype">
		<property name="name" value="testEditor11" />
		<property name="active" value="true" />
		<property name="id" value="id13" />
	</bean>
	<bean id="ping" class="org.jacp.swing.demo1.editors.BGPing" scope="prototype">
		<property name="name" value="ping" />
		<property name="active" value="true" />
		<property name="id" value="id13" />
	</bean>

	<bean id="pong" class="org.jacp.swing.demo1.editors.BGPong" scope="prototype">
		<property name="name" value="pong" />
		<property name="active" value="true" />
		<property name="id" value="id14" />
	</bean>

	<bean id="testBean" class="org.jacp.swing.demo1.editors.TestBean"
		scope="prototype">
	</bean>


</beans>

